{
  "compilerOptions": {
    "incremental": true,
    "outDir": "./build/",
    "lib": [
      "ES2023",
      "ES2023.Array",
      "ES2023.Collection"
    ],
    "target": "ES6",
    "module": "Node16",
    "moduleResolution": "Node16",
    "esModuleInterop": true,
    "strict": true,
    "allowJs": true,
    "noImplicitReturns": true,
    "noImplicitAny": false,
    "noFallthroughCasesInSwitch": true,
    "declaration": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "noEmitOnError": true,
    "skipLibCheck": true,
  },
  "include": [
    "src/**/*.ts"
  ]
}